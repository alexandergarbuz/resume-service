# This is for running docker with environment variables like this 
#
#  docker build -t alexandergarbuz/resume-service .
#
#  docker run -it -p 80:80 --env-file="./.env.local"  --name=resume-service-local alexandergarbuz/resume-service
#
WEB_SERVER_PORT=80
WEB_SERVER_CONTEXT_PATH=/

MYSQL_URL=jdbc:mysql://host-name:3306/resume_manager_db
MYSQL_USER=db_user
MYSQL_PASSWORD=db_password

SPRING_PROFILES_ACTIVE=default
